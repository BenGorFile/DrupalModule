{
    "name": "bengor-file/bengor_file",
    "description": "Drupal module to connect BenGorFile and Drupal",
    "keywords": ["file", "drupal", "module", "ddd"],
    "license": "MIT",
    "type": "drupal-module",
    "authors": [
        {
            "name": "Beñat Espiña Diaz",
            "email": "benatespina@gmail.com",
            "homepage": "https://benatespina.com"
        },
        {
            "name": "Gorka Laucirica Ibarra",
            "email": "gorka.lauzirika@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/client-common": "^1.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix --config=.php_cs",
            "php-cs-fixer fix --config=.phpspec_cs"
        ],
        "test": "phpspec run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
